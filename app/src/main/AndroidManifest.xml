<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="dev.zgmgmm.esls">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:name=".ESLS"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"

    >

        <activity android:name=".LoginActivity">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER"/>
                <action android:name="android.intent.action.VIEW"/>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity"/>
        <activity android:name=".GoodManageActivity"/>
        <activity android:name=".GoodInfoActivity"/>
        <activity android:name=".LabelManageActivity"/>
        <activity android:name=".LabelInfoActivity"/>
        <activity android:name=".BindActivity"/>
    </application>

</manifest>